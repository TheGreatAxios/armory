{
  "name": "@armory-sh/base",
  "version": "0.2.35",
  "license": "MIT",
  "author": "Sawyer Cutler <sawyer@dirtroad.dev>",
  "keywords": [
    "x402",
    "ai",
    "agentic commerce",
    "ai agent",
    "stablecoins",
    "eip-3009",
    "skale",
    "base",
    "machine economy",
    "payment",
    "crypto",
    "web3",
    "ethereum",
    "usdc"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "bun": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types/hooks": {
      "types": "./src/types/hooks.ts",
      "bun": "./src/types/hooks.ts",
      "default": "./src/types/hooks.ts"
    },
    "./client-hooks-runtime": {
      "types": "./src/client-hooks-runtime.ts",
      "bun": "./src/client-hooks-runtime.ts",
      "default": "./src/client-hooks-runtime.ts"
    },
    "./dist/*": "./dist/*.js"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheGreatAxios/armory.git",
    "directory": "packages/base"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^25.2.1",
    "bun-types": "latest",
    "typescript": "5.9.3"
  },
  "scripts": {
    "build": "rm -rf dist && tsup && tsc --emitDeclarationOnly",
    "lint": "bun run build",
    "format": "bun run lint",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  }
}
