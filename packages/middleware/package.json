{
  "name": "@armory-sh/middleware",
  "version": "0.2.2",
  "license": "MIT",
  "author": "Sawyer Cutler <sawyer@dirtroad.dev>",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "bun": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./integrations/hono": {
      "types": "./integrations/hono.ts",
      "default": "./integrations/hono.ts"
    },
    "./integrations/hono-simple": {
      "types": "./integrations/hono-simple.ts",
      "default": "./integrations/hono-simple.ts"
    },
    "./integrations/express": {
      "types": "./integrations/express.ts",
      "default": "./integrations/express.ts"
    },
    "./integrations/elysia": {
      "types": "./integrations/elysia.ts",
      "default": "./integrations/elysia.ts"
    },
    "./dist/*": "./dist/*.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thegreataxios/armory.git",
    "directory": "packages/middleware"
  },
  "dependencies": {
    "@armory-sh/base": "workspace:*",
    "@armory-sh/facilitator": "workspace:*"
  },
  "devDependencies": {
    "typescript": "5.9.3",
    "bun-types": "latest"
  },
  "scripts": {
    "build": "tsup",
    "test": "bun test"
  }
}
